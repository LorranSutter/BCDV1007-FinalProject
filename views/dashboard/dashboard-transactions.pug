div(class="transaction-history")
    h3 Transaction history
    div(class="transaction-history-content")
        table(class="table-transaction-history")
            thead
                tr
                    th(id="col-seller") Seller
                    th(id="col-buyer") Buyer
                    th(id="col-date") Date
                    th(id="col-asset") Item
                    th(id="col-amount") Amount
            tbody
                each i in [1,2,3,4,5,6,7,8,9]
                    tr
                        td Hash !{i}
                        td Timestamp !{i}